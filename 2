#include <stdio.h>

int main()
{
    int s,a;
    printf("Enter the size of array: ");
    scanf("%d", &s);
    int arr[s];
    printf("Enter the elements of the array[5]: ");
    for(int i = 0; i < s; i++)
        {
        scanf("%d", &arr[i]);
        }

    for(int i = 0; i < s; i++)
        {
        for(int j = i+1; j < s; j++)
        {
            if(arr[i] < arr[j])
               {
                   a = arr[i];
                   arr[i] = arr[j];
                   arr[j] = a;
               }
        }
        }
        printf("The second largest number: %d", arr[1]);


}
